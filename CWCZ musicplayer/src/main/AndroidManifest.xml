<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.music_list2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Music_list2">
        <activity
            android:name="com.example.music_list2.MusicActivity"
            android:exported="false" />

        <service
            android:name="com.example.music_list2.MusicService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name="com.example.music_list2.MainActivity"
            android:exported="true">
           <!-- <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>-->
        </activity>
        <activity android:name="com.example.music_list2.LoginActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.music_list2.RegisterActivity"
            android:exported="false"
            android:windowSoftInputMode="adjustPan|stateHidden"/>
    </application>

</manifest>